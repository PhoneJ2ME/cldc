<configuration>
<constants>

<!--
    Indentifiers for Suite Storage subsystem
-->
<constant_class Package="com.sun.midp.configurator" Name="Constants" Scope="public"
    Comment="/** Indentifiers for the MIDP Storage subsystem */">
 <constant Type="int"
            Name="INTERNAL_STORAGE_ID"
            Value="0"
            NativeOnly="false"
            Comment="Storage ID for the internal storage."/>
 <constant Type="int"
            Name="UNUSED_STORAGE_ID"
            Value="(-1)"
            NativeOnly="false"
            Comment="Unused storage ID."/>
<constant Type="int"
            Name="MAX_STORAGE_NUM"
            Value="1"
            NativeOnly="false"
            Comment="Number of the supported storages: 2 for internal and only one external.
 This value should be changed if more than one external storage is supported."/>
 </constant_class>

</constants>
</configuration>
