# Javadoc source path
SUBSYSTEM_PIPE_SOURCEPATH += $(SUBSYSTEM_DIR)/protocol/pipe/mvm/classes

SUBSYSTEM_PIPE_PACKAGES += \
    com.sun.midp.io.j2me.pipe.serviceProtocol

# Java files for the ( pipe ) module
